class Solution
{
    public int solution(int []A, int []B)
    {
        int answer = Integer.MAX_VALUE;
        // A배열의 모든 경우의 수를 교체하는 작업 필요

        int sum = 0;
        for(int i = 0; i < A.length; i++) {
            sum += A[i] * B[i];
        }
        answer = Math.min(answer, sum);
        return answer;
    }
}

// A 배열의 모든 경우의 수를 나타내는데 시간효율성이 만족되지 않아 못 풀었음